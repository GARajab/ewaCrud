  <body>
   <nav>
  <div class="container">
    <div class="navbar">
      <h1 id="nav-header">EWA "Mohamed Rajab"</h1>
      <div class="nav-links">
      

        <a
          href="/auth/sign-out"
          class="btn btn-danger"
          onclick="clearLoginNotification()"
          >Sign Out</a
        >
        <a
          href="/services/dashBoard"
          class="btn btn-danger"
          onclick="clearLoginNotification()"
          >Home</a
        >
        <a
          href="/schemes"
          class="btn btn-danger"
          onclick="clearLoginNotification()"
          >All Schemes</a
        >
      
        <a
          href="/auth/sign-out"
          class="btn btn-primary"
          onclick="clearLoginNotification()"
          >Sign Out</a
        >
        <a
          href="/schemes/dashBoard"
          class="btn btn-primary"
          onclick="clearLoginNotification()"
          >Home</a
        >
        <a
          href=""
          class="btn btn-primary"
          onclick="clearLoginNotification()"
          >My Services</a
        >

        <a href="/auth/sign-in" class="btn btn-primary">Sign In</a>
        <a href="/auth/sign-up" class="btn btn-primary">Sign Up</a>
      
      </div>
    </div>
  </div>
</nav>

    <h1 style="text-align: center">Services Table</h1>
    <div class="col-md-14 text-center">
      <br />
      <div class="d-md-flex justify-content-md-end">
        <a
          id="addBtn"
          href="/carriers"
          class="btn col-2 btn-lg btn-success mybtn btn-block tx-tfm"
        >
          + Add
        </a>
   
        <a
          href="/orders/new"
          class="btn col-2 btn-lg btn-primary mybtn btn-block tx-tfm"
          style="margin-left: 10px"
        >
          Add New Order
        </a>
      </div>
      <br />
    </div>

    <table class="table table-bordered table-hover" style="text-align: center">
      <thead class="thead-dark">
        <tr>
          <th scope="col">#</th>
          <th scope="col">Scheme Reference</th>
          <th scope="col">Application Number</th>
          <th scope="col">Contractor Name</th>
          <th scope="col">Area</th>

          <th scope="col" class="col-md-2">Action</th>
        
        </tr>
      </thead>
      <tbody>
         <% createSchemes.forEach((createScheme, index) => { %>
        <tr>
          <th scope="row"><%= index+1 %></th>
          <td><%= createScheme.Sch_Ref %></td>
          <td><%= createScheme.APPNUMBER %></td>
          <td><%= createScheme.ContractorName %></td>
          <td><%= createScheme.Area %></td>

          <div class="btn-group text-center">
            <td>
              <a
                href=""
                class="btn btn-primary btn-block tx-tfm btn-sm"
                >Edit</a
              >
              <br />
              <form
                action=""
                method="POST"
                style="display: inline"
              >
                <button
                  type="submit"
                  class="btn btn-block btn-danger tx-tfm btn-sm"
                  onclick="return confirm('Are you sure you want to delete this item?');"
                >
                  Delete
                </button>
           
              </form>
               
            </td>
          </div>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
